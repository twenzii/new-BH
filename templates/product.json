               remainingSpotsElement.textContent = window.globalTimer.currentCount;\n            }\n        };\n\n        window.globalTimer.runTimer = function() {\n            const elapsedTime = Date.now() - window.globalTimer.startTime;\n            for (let i = window.globalTimer.updates.length - 1; i >= 0; i--) {\n                if (elapsedTime >= window.globalTimer.updates[i].delay) {\n                    window.globalTimer.currentCount = window.globalTimer.updates[i].count;\n                    break;\n                }\n            }\n            window.globalTimer.updateDisplay();\n        };\n\n        window.globalTimer.intervalId = setInterval(window.globalTimer.runTimer, 1000);\n    }\n\n    // Immediate update on script load\n    window.globalTimer.updateDisplay();\n\n    // Update on DOM content loaded\n    document.addEventListener('DOMContentLoaded', window.globalTimer.updateDisplay);\n\n    // Update on window load\n    window.addEventListener('load', window.globalTimer.updateDisplay);\n    </script>\n\n    <style>\n    #remaining-spots {\n        color: #C62828;\n    }\n    </style>\n  </div>\n</div>\n            </div>"
