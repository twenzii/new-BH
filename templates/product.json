.cc-btn{\n    padding:12px 14px;\n    border:none;\n    border-radius:10px;\n    cursor:pointer;\n    font-size:14px;\n    font-weight:700;\n    background:#111;\n    color:#fff;\n    height:44px;\n    white-space:nowrap;\n    grid-column: 1 / -1;\n  }\n\n  .cc-result{\n    margin-top:14px;\n    padding:12px;\n    border-radius:12px;\n    background:#fafafa;\n    border:1px solid #eee;\n  }\n  .cc-badge{\n    display:inline-block;\n    padding:8px 12px;\n    border-radius:999px;\n    background:#111;\n    color:#fff;\n    font-weight:800;\n    font-size:14px;\n    margin-bottom:8px;\n  }\n  .cc-note{ margin:0 0 6px; color:#333; font-size:14px; }\n  .cc-small{ margin:0; color:#666; font-size:13px; line-height:1.35; }\n  .cc-disclaimer{ margin:12px 0 0; color:#888; font-size:12px; }\n\n  @media (max-width: 749px){\n    .cc-grid{ grid-template-columns: 1fr; }\n    .cc-btn{ width:100%; }\n  }\n</style>\n\n<script>\n(function(){\n  const btn = document.getElementById(\"ccCalcBtn\");\n  const hEl = document.getElementById(\"ccHeight\");\n  const wEl = document.getElementById(\"ccWeight\");\n\n  const result = document.getElementById(\"ccResult\");\n  const badge  = document.getElementById(\"ccSizeBadge\");\n  const note   = document.getElementById(\"ccNoteText\");\n  const why    = document.getElementById(\"ccWhyText\");\n\n  if(!btn) return;\n\n  const clamp = (v,min,max)=>Math.max(min,Math.min(max,v));\n\n  function recommendSizeFromBMI(heightCm, weightKg){\n    const h = heightCm / 100;\n    const bmi = weightKg / (h*h);\n\n    // BMI -> size mapping (EDIT THESE IF NEEDED)\n    if (bmi < 19.5) return { size:\"XS\", bmi };\n    if (bmi < 22.0) return { size:\"S\",  bmi };\n    if (bmi < 25.0) return { size:\"M\",  bmi };\n    if (bmi < 28.5) return { size:\"L\",  bmi };\n    if (bmi < 32.0) return { size:\"XL\", bmi };\n    if (bmi < 35.5) return { size:\"2XL\",bmi };\n    return { size:\"3XL (XXXL)\", bmi };\n  }\n\n  btn.addEventListener(\"click\", function(){\n    const height = parseFloat(hEl.value);\n    const weight = parseFloat(wEl.value);\n\n    if(!height || !weight){\n      result.style.display = \"block\";\n      badge.textContent = \"Please enter height + weight\";\n      note.textContent = \"Add your height (cm) and weight (kg) to get a recommendation.\";\n      why.textContent = \"\";\n      return;\n    }\n\n    const heightC = clamp(height, 120, 220);\n    const weightC = clamp(weight, 30, 200);\n\n    const rec = recommendSizeFromBMI(heightC, weightC);\n\n    result.style.display = \"block\";\n    badge.textContent = \"Recommended Size: \" + rec.size;\n    note.textContent = \"This is a quick estimate based on height and weight.\";\n    why.textContent = \"Height: \" + heightC + \" cm ‚Ä¢ Weight: \" + weightC + \" kg ‚Ä¢ BMI: \" + rec.bmi.toFixed(1) + \".\";\n  });\n})();\n</script>\n</div>"
